////////////////////////////////////////////////////////////////////////////////////////////
// Output
////////////////////////////////////////////////////////////////////////////////////////////
RWTexture2D<float4> outputTexture : register(u0);

////////////////////////////////////////////////////////////////////////////////////////////
// main
////////////////////////////////////////////////////////////////////////////////////////////
[numthreads(16, 16, 1)]
void main(uint3 dispatchThreadId : SV_DispatchThreadID) {
	
	outputTexture[dispatchThreadId.xy] = float4(dispatchThreadId.x / 256.0f, dispatchThreadId.y / 256.0f, 0.0f, 1.0f);
	
}